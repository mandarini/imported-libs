{
  "name": "webcomponents-webpack",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/webcomponents-webpack/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/webcomponents-webpack",
        "compiler": "babel",
        "main": "apps/webcomponents-webpack/src/main.ts",
        "tsConfig": "apps/webcomponents-webpack/tsconfig.app.json",
        "webpackConfig": "apps/webcomponents-webpack/webpack.config.js",
        "assets": [
          "apps/webcomponents-webpack/src/favicon.ico",
          "apps/webcomponents-webpack/src/assets"
        ],
        "index": "apps/webcomponents-webpack/src/index.html",
        "baseHref": "/",
        "styles": ["apps/webcomponents-webpack/src/styles.css"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "fileReplacements": [
            {
              "replace": "apps/webcomponents-webpack/src/environments/environment.ts",
              "with": "apps/webcomponents-webpack/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/webpack:dev-server",
      "options": {
        "buildTarget": "webcomponents-webpack:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "webcomponents-webpack:build:production"
        }
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/apps/webcomponents-webpack"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/webcomponents-webpack"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/webcomponents-webpack/**/*.ts"]
      }
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/web-components",
        "port": 4400,
        "configDir": "apps/webcomponents-webpack/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nrwl/storybook:build",
      "outputs": ["{options.outputDir}"],
      "options": {
        "uiFramework": "@storybook/web-components",
        "outputDir": "dist/storybook/webcomponents-webpack",
        "configDir": "apps/webcomponents-webpack/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  }
}
